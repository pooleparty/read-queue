{
  "manifest_version": 2,

  "name": "Queue",
  "description": "This extension allows the user to save pages to read in a queue.",
  "version": "1.0",

  "browser_action": {
    // "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["public/bundles/background.js"],
    "persistent": true
  },

  "permissions": ["activeTab", "storage", "contextMenus"]
}
